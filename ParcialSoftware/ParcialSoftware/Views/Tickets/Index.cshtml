@model IEnumerable<ParcialSoftware.Data.Ticket>
@{
    ViewData["Title"] = "Index";

}
 
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" 
    rel="stylesheet" 
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" 
    crossorigin="anonymous">
    

</head>
<body>
    <h1>Registro de Boletos</h1><br>
    
    <div class="d-inline-block p-3 mb-2 bg-success text-white">
    <a action="/Ticket/Ticket" method="POST">
      <div class="form-group">
        <label  for="nombre ">Ingrese su nombre :</label>
        <div class="row g-3">
        <div class="col-sm-10"> <input class="form-control" 
        type="text" placeholder="Escribe tu nombre " required
         data-pristine-required-message="Este campo es obligatorio"></div>
      
      </div><br>
       <div class="form-group">
        <label  for="nombre ">Ingrese su documento de identidad :</label>
        <div class="row g-3">
        <div class="col-sm-10"> <input class="form-control" 
        type="text" placeholder="Escribe tu nombre " required
         data-pristine-required-message="Este campo es obligatorio"></div>
      
      </div><br>
           
        
      <label>Entradas</label>
      <select class="form-control" name="" id="" >
        <option value="">Norte</option>
         <option value="">Sur</option>
         <option value="">Oeste</option>
         <option value="">Este</option>
        
      </select></div><br>
     
      <div>
      </select></div><br><br>
      <div class="form-group">
        <label>Fecha de creación</label><br>
        <input class="form-control" type="datetime-local" name="hora de creación" id="date" required></div><br>
      <div class="form-group">
      <input  type="submit" value="Enviar" class="btn-primary" required></div>
        <br>
      <div class="form-group">
      <input type="reset" value="Limpiar" class="btn-primary" ></div><br>
      <br>
      </div>
    
      </a>
      
    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
     integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" 
    crossorigin="anonymous"></script></div>
    <script src="js/pristine.min.js"></script>
    <script src="js/formulario.js"></script>

    
</body>
</html>
